<div class="All-content">
   <h1 class="heading">Create New Adress</h1>
   <div class="address-content address-container">
    <%= form_with(url: address_index_path , method: "post") do |form|%>
      <%= render partial: "addaddress" , locals: {
        form: form
        }
      %>
      <div class="col-12">
        <%= form.submit "Add Address",
            class: "btn btn-primary"
        %>
      </div>
    <%end%>
  </div>
</div>
